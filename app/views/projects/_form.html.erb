<%= form_for(project, url: project.new_record? ? team_projects_path(team) : team_project_path(team, project)) do |f| %>

  <div class="form-group">
    <%= f.label :name, '项目名称' %>
    <%= f.text_field :name, class: 'form-control'%>
  </div>
  <% User.all.each do |d| %>
  <div class="checkbox">
    <label>
     <%= check_box_tag "user[]", d.id %>
     <%= d.name %>
    </label>
  </div>
  <% end %>
  <button type="submit" class="btn btn-default">创建</button>
<% end %>
